
<%= form_with(model: @post, local: true) do |form| %>

    <div class="match">
      <%= form.label :match %>
      <%= form.text_field :match %>
    </div>
  
    <div class="time">
      <%= form.label :start_time %>
      <%= form.datetime_select :start_time %>
    </div>
  
  
  
    <div class="submit">
      <%= form.submit %>
    </div>
  
  <% end %>